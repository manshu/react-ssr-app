{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "prod": "npm-run-all --parallel prod:*",
    "prod:build": "node build/bundle.js",
    "prod:build-server": "webpack --config webpack.server.js",
    "prod:build-client": "webpack --config webpack.client.js",
    "prod:build-css": "postcss src/assets/css/app.css -o public/css/app.css",
    "dev": "npm-run-all --parallel dev:*",
    "dev:watch-css": "postcss src/assets/css/app.css -o public/css/app.css -w",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.4",
    "axios": "0.21.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "css-loader": "^3.4.0",
    "express": "4.17.3",
    "express-http-proxy": "1.0.6",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "4.1.1",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^4.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^5.2.1",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "5.2.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "^2.1.1",
    "style-loader": "^1.1.2",
    "tailwindcss": "^3.0.24",
    "webpack": "3.5.6",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
